<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>成功</title>
  <!-- Import style -->
  <link
    rel="stylesheet"
    href="//cdn.jsdelivr.net/npm/element-plus/dist/index.css"
  />
  <!-- Import Vue 3 -->
  <script src="//cdn.jsdelivr.net/npm/vue@3"></script>
  <!-- Import component library -->
  <script src="//cdn.jsdelivr.net/npm/element-plus"></script>
  <style>
    .h-50{
      height: 50px;
    }
  </style>
</head>
<body>
  <div id="app">
    <el-result
      icon="success"
      title="<%= msg %>"
      sub-title="您已<%= msg %>，欢迎登录！"
    >
      <template #extra>
        <el-link type="primary" href="<%= url %>">{{ message }}</el-link>
      </template>
    </el-result>
  </div>
</body>
<script>
  // 使用 Vue 的全局 API
  const { createApp, ref } = Vue;

  const App = {
    setup() {
      // 使用 Vue.ref() 创建响应式变量
      const message = ref('返回登录页');
      return { message };
    },
  };

  const app = createApp(App);
  app.use(ElementPlus);
  app.mount('#app');
</script>
</html>